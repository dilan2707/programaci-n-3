public class suma {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int resultadossuma =0;
        
        for(int contador=1; contador<100000000;contador++){
            if(contador %2 !=0){
                resultadossuma=resultadossuma+contador;
            }
           
        }
        System.out.println("resultadossuma"+resultadossuma);
    }
    
}
